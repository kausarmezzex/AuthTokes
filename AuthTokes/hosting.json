{
  "Logging": {
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Debug",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "Host": {
    "EnvironmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Production",
      "SOME_VAR": "some-value"
    }
  },
  "WebJobs": {
    "EnableWebJobs": true,
    "WebJobs": [
      {
        "Name": "MyJob",
        "Handler": "MyJobHandler"
      }
    ]
  }
}